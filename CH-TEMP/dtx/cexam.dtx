    {
      \cexam_sep_isin:nn {#1}{$$}%$$
    }
    {
      \str_if_in:nnTF {#1}{\[}%\]
	{
	  \cexam_sep_isin:nn {#1}{\[}%\]
	  }
	  {
	    \str_if_in:nnTF {#1}{\begin}%\end
	    {
	      \cexam_sep_isin:nn {#1}{\begin}%\end
	    }
	    {
	       \tl_set:Nn \sep_hd_tl {#1}
	       \tl_set:Nn \sep_bd_tl {}
	       \tl_set:Nn \sep_tl_tl {}
	      \bool_set_false:N \g_@@_sep_tl_bool

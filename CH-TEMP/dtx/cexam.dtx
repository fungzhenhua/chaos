      \cs_new:Npn \cexam_sep_tikz:p #1\begin#2#3\end#4#5 \scan_stop:
      {
	 \str_if_in:nnTF {#2}{tikzpicture}
	 { 
	    \bool_set_false:N \cexam_nopic_bool
	    \bool_set_true:N \cexam_notab_bool

  \hbox_set:Nn \blank_wd_box {#1}
  \dim_set:Nn \blank_wd_dim {\box_wd:N \blank_wd_box}
  \dim_add:Nn \blank_wd_dim {2\cexam_ccwd_dim}
  \hspace{3pt}
  \dim_while_do:nNnn
  {\blank_wd_dim} > {0pt}
  { 
    \dim_sub:Nn \blank_wd_dim {\cexam_ccwd_dim}
    \cexam_quad_tl 
    \hspace{-13pt}
    \quad
  }
  \hspace{6pt}
}

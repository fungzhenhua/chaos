%
%    \begin{macrocode}
  \cs_new:Npn \cexam_get_rec:nnnnnn #1#2#3#4#5#6
  {
%    \end{macrocode}
% 置空存储头部
%    \begin{macrocode}
    \tl_set:Nn \sep_HD_tl {}
%    \end{macrocode}
% 获得排版宽度
%    \begin{macrocode}
    \dim_set:Nn \get_rec_linewd_dim{\linewidth}
    \dim_sub:Nn \get_rec_linewd_dim{#3}
    \dim_sub:Nn \get_rec_linewd_dim{#4}
